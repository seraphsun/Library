<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="refresh_01">
        <attr name="refreshType" format="enum">
            <enum name="material" value="0" />
            <enum name="circles" value="1" />
            <enum name="water_drop" value="2" />
            <enum name="ring" value="3" />
            <enum name="smartisan" value="4" />
        </attr>
        <attr name="refreshColor" format="reference" />
        <attr name="refreshColors" format="reference" />
    </declare-styleable>

    <declare-styleable name="refresh_02">
        <attr name="AniBackColor" format="color" />
        <attr name="AniForeColor" format="color" />
        <attr name="CircleSmaller" format="integer" />
    </declare-styleable>

    <declare-styleable name="refresh_03">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="jellyColor" format="color" />
    </declare-styleable>

    <declare-styleable name="refresh_04">
        <attr name="game_type" format="enum">
            <enum name="hit_block" value="0" />
            <enum name="battle_city" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FunGame">
        <attr name="left_model_color" format="color" />
        <attr name="middle_model_color" format="color" />
        <attr name="right_model_color" format="color" />
    </declare-styleable>
    <declare-styleable name="HitBlock">
        <attr name="block_horizontal_num" format="integer" />
        <attr name="ball_speed" format="integer">
            <enum name="low" value="3" />
            <enum name="medium" value="6" />
            <enum name="fast" value="9" />
        </attr>
    </declare-styleable>

    <declare-styleable name="refresh_9">
        <attr name="clwl_radius" format="dimension" />
        <attr name="clwl_gap" format="dimension" />
        <attr name="clwl_circleColor" format="color" />
        <attr name="clwl_collapseDuration" format="integer" />
        <attr name="clwl_rippleDuration" format="integer" />
        <attr name="clwl_gummyDuration" format="integer" />
        <attr name="clwl_maxHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="refresh_10">
        <attr name="indicator">
            <flag name="BallPulse" value="0" />
            <flag name="BallGridPulse" value="1" />
            <flag name="BallClipRotate" value="2" />
            <flag name="BallClipRotatePulse" value="3" />
            <flag name="SquareSpin" value="4" />
            <flag name="BallClipRotateMultiple" value="5" />
            <flag name="BallPulseRise" value="6" />
            <flag name="BallRotate" value="7" />
            <flag name="CubeTransition" value="8" />
            <flag name="BallZigZag" value="9" />
            <flag name="BallZigZagDeflect" value="10" />
            <flag name="BallTrianglePath" value="11" />
            <flag name="BallScale" value="12" />
            <flag name="LineScale" value="13" />
            <flag name="LineScaleParty" value="14" />
            <flag name="BallScaleMultiple" value="15" />
            <flag name="BallPulseSync" value="16" />
            <flag name="BallBeat" value="17" />
            <flag name="LineScalePulseOut" value="18" />
            <flag name="LineScalePulseOutRapid" value="19" />
            <flag name="BallScaleRipple" value="20" />
            <flag name="BallScaleRippleMultiple" value="21" />
            <flag name="BallSpinFadeLoader" value="22" />
            <flag name="LineSpinFadeLoader" value="23" />
            <flag name="TriangleSkewSpin" value="24" />
            <flag name="Pacman" value="25" />
            <flag name="BallGridBeat" value="26" />
            <flag name="SemiCircleSpin" value="27" />
        </attr>
        <attr name="indicator_color" format="color" />
    </declare-styleable>

    <declare-styleable name="refresh_11">
        <attr name="water_drop_color" format="color" />
        <attr name="max_circle_radius" format="dimension" />
        <attr name="min_circle_radius" format="dimension" />
        <attr name="top_circle_x" format="dimension" />
        <attr name="top_circle_y" format="dimension" />
        <attr name="bottom_circle_x" format="dimension" />
        <attr name="bottom_circle_y" format="dimension" />
    </declare-styleable>
</resources>